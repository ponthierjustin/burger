<h1>EAT-DA-BURGER</h1>



<ul>
  {{#each burgers}}
  <li>{{burger_name}}</li>
 
  {{/each}}
</ul>

<h2>ADD A BURGER</h2>
<form class="create-form">

  <div class="form-group">
    <label for="new">Burger Name:</label>
    <input type="text" id="new" name="burger_name">
  </div>


  <button type="submit">Add Burger</button>
</form>